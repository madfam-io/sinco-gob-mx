{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "url": ["http://127.0.0.1:8000/"]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "is-on-https": "off",
        "redirects-http": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
